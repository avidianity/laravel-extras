{
    "name": "avidianity/laravel-extras",
    "description": "Extra powers for faster development in Laravel",
    "keywords": [
        "avidianity",
        "laravel",
        "laravel-extras"
    ],
    "homepage": "https://github.com/avidianity/laravel-extras",
    "license": "MIT",
    "authors": [
        {
            "name": "John Michael Manlupig",
            "email": "manlupigjohnmichael@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/auth": "^9.0",
        "illuminate/contracts": "^9.0",
        "illuminate/database": "^9.0",
        "illuminate/support": "^9.0",
        "illuminate/notifications": "^9.0",
        "illuminate/validation": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Avidianity\\LaravelExtras\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Avidianity\\LaravelExtras\\Providers\\LaravelExtrasServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
